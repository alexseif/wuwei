{% extends 'base.html.twig' %}

{% block h1 %}CostOfLife
{% endblock %}
{% block card_actions %}
	<div>
		<a href="{{ path('app_cost_of_life_index') }}">back to list</a>
		<a href="{{ path('app_cost_of_life_edit', {'id': cost_of_life.id}) }}">edit</a>
		{{ include('cost_of_life/_delete_form.html.twig') }}
	</div>
{% endblock %}
{% block body %}
	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ cost_of_life.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ cost_of_life.name }}</td>
			</tr>
			<tr>
				<th>Value</th>
				<td>{{ cost_of_life.value }}</td>
			</tr>
			<tr>
				<th>CreatedAt</th>
				<td>{{ cost_of_life.createdAt ? cost_of_life.createdAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>UpdatedAt</th>
				<td>{{ cost_of_life.updatedAt ? cost_of_life.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>
{% endblock %}
