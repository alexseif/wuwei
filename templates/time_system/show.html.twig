{% extends 'base.html.twig' %}

{% block title %}TimeSystem{% endblock %}

{% block body %}
    <h1>TimeSystem</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ time_system.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ time_system.name }}</td>
            </tr>
            <tr>
                <th>From_time</th>
                <td>{{ time_system.fromTime ? time_system.fromTime|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>To_time</th>
                <td>{{ time_system.toTime ? time_system.toTime|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Tags</th>
                <td>{{ time_system.tags }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ time_system.createdAt ? time_system.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ time_system.updatedAt ? time_system.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_time_system_index') }}">back to list</a>

    <a href="{{ path('app_time_system_edit', {'id': time_system.id}) }}">edit</a>

    {{ include('time_system/_delete_form.html.twig') }}
{% endblock %}
