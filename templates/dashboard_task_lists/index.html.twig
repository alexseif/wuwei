{% extends 'base.html.twig' %}

{% block h1 %}DashboardTaskLists index
{% endblock %}
{% block card_actions %}
	{{ include('components/Link/New.html.twig', {'name': 'dashboard_task_lists'}) }}

{% endblock %}
{% block body %}
	<table class="table table-striped">
		<thead>
			<tr>
				<th>TaskList</th>
				<th>Timestamps</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			{% for dashboard_task_list in dashboard_task_lists %}
				<tr>
					<td>
						{{ include('components/Link.html.twig', {'text': dashboard_task_list.taskList.name, 'link': path('app_dashboard_task_lists_show', {'id': dashboard_task_list.id}), 'class': 'btn btn-sm'}) }}
					</td>
					<td>
						{{ include('components/Badge/Timestamps.html.twig', {'entity': dashboard_task_list}) }}
					</td>
					<td>
						{{ include('components/Link/Edit.html.twig', {'name': 'dashboard_task_lists', 'id': dashboard_task_list.id}) }}
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="3">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>
{% endblock %}
