{% extends 'base.html.twig' %}

{% block h1 %}Currency
{% endblock %}
{% block card_actions %}
	<div>
		<a href="{{ path('app_currency_index') }}">back to list</a>
		<a href="{{ path('app_currency_edit', {'id': currency.id}) }}">edit</a>
		{{ include('currency/_delete_form.html.twig') }}
	</div>
{% endblock %}
{% block body %}
	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ currency.id }}</td>
			</tr>
			<tr>
				<th>Code</th>
				<td>{{ currency.code }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ currency.name }}</td>
			</tr>
			<tr>
				<th>EGP</th>
				<td>{{ currency.EGP }}</td>
			</tr>
			<tr>
				<th>CreatedAt</th>
				<td>{{ currency.createdAt ? currency.createdAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>UpdatedAt</th>
				<td>{{ currency.updatedAt ? currency.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>
{% endblock %}
