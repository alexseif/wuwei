{% set body %}
<table class="table table-striped">
	<tbody>
		<tr>
			<th>Id</th>
			<td>{{ work_log.id }}</td>
		</tr>
		<tr>
			<th>Name</th>
			<td>{{ work_log.name }}</td>
		</tr>
		<tr>
			<th>Duration</th>
			<td>{{ work_log.duration }}</td>
		</tr>
		<tr>
			<th>PricePerUnit</th>
			<td>{{ work_log.pricePerUnit }}</td>
		</tr>
		<tr>
			<th>Total</th>
			<td>{{ work_log.total }}</td>
		</tr>
		<tr>
			<th>CreatedAt</th>
			<td>{{ work_log.createdAt ? work_log.createdAt|date('Y-m-d H:i:s') : '' }}</td>
		</tr>
		<tr>
			<th>UpdatedAt</th>
			<td>{{ work_log.updatedAt ? work_log.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
		</tr>
	</tbody>
</table>
{% endset %}
{{ include('components/Crud/Show.html.twig', {
    'entity_name': entity_name,
    'entity_title': entity_title,
    'entity': work_log,
    'body': body
    }) }}
