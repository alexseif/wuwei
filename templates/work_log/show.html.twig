{% extends 'base.html.twig' %}

{% block h1 %}WorkLog
{% endblock %}
{% block card_actions %}
	<div>
		<a href="{{ path('app_work_log_index') }}">back to list</a>
		<a href="{{ path('app_work_log_edit', {'id': work_log.id}) }}">edit</a>
		{{ include('work_log/_delete_form.html.twig') }}
	</div>
{% endblock %}
{% block body %}
	<table class="table table-striped">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ work_log.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ work_log.name }}</td>
			</tr>
			<tr>
				<th>Duration</th>
				<td>{{ work_log.duration }}</td>
			</tr>
			<tr>
				<th>PricePerUnit</th>
				<td>{{ work_log.pricePerUnit }}</td>
			</tr>
			<tr>
				<th>Total</th>
				<td>{{ work_log.total }}</td>
			</tr>
			<tr>
				<th>CreatedAt</th>
				<td>{{ work_log.createdAt ? work_log.createdAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>UpdatedAt</th>
				<td>{{ work_log.updatedAt ? work_log.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>
{% endblock %}
