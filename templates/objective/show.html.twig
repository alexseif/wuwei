{% extends 'base.html.twig' %}

{% block h1 %}Objective
{% endblock %}
{% block card_actions %}
	<div>
		<a href="{{ path('app_objective_index') }}">back to list</a>
		<a href="{{ path('app_objective_edit', {'id': objective.id}) }}">edit</a>
		{{ include('objective/_delete_form.html.twig') }}
	</div>
{% endblock %}
{% block body %}
	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ objective.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ objective.name }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ objective.description }}</td>
			</tr>
			<tr>
				<th>CreatedAt</th>
				<td>{{ objective.createdAt ? objective.createdAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>UpdatedAt</th>
				<td>{{ objective.updatedAt ? objective.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>
{% endblock %}
