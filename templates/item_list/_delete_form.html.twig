<form method="post" action="{{ path('app_item_list_delete', {'id': item_list.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ item_list.id) }}">
	<button class="btn btn-sm btn-link text-danger">Delete</button>
</form>
